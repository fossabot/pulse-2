{
    "go.goroot": "${workspaceFolder}/bazel-pulse/external/rules_go~~go_sdk~pulse__download_0",
    "go.toolsEnvVars": {
        "GOPACKAGESDRIVER": "${workspaceFolder}/tools/gopackagesdriver.sh"
    },
    "go.enableCodeLens": {
        "runtest": false
    },
    "gopls": {
        "build.directoryFilters": [
            "-bazel-bin",
            "-bazel-out",
            "-bazel-testlogs",
            "-bazel-pulse-v12"
        ],
        "formatting.gofumpt": true,
        "formatting.local": "github.com/machanirobotics/pulse",
        "ui.completion.usePlaceholders": true,
        "ui.semanticTokens": true,
        "ui.codelenses": {
            "gc_details": false,
            "regenerate_cgo": false,
            "generate": false,
            "test": false,
            "tidy": false,
            "upgrade_dependency": false,
            "vendor": false
        }
    },
    "go.useLanguageServer": true,
    "go.lintOnSave": "off",
    "go.vetOnSave": "off",
    "editor.fontLigatures": false,
    "[yaml]": {
        "editor.tabSize": 2
    },
    "[json]": {
        "editor.tabSize": 4
    },
    "files.associations": {
        "*.bzlmod": "bazel"
    },
    "files.insertFinalNewline": true,
}